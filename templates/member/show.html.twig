<h1>Fiche Membre</h1>
<p>Nom: {{ member.name }}</p>
<p>Email: {{ member.email }}</p>

<h2>Inventaires</h2>
<ul>
    {% for inventory in member.inventories %}
        <li>
            <a href="{{ path('app_inventory_show', {'id': inventory.id}) }}">{{ inventory.name }}</a>
        </li>
    {% endfor %}
</ul>
